@article{balfry_efficacy_2006,
	title = {Efficacy of an equal blend of canola oil and poultry fat as an alternate dietary lipid source for {Atlantic} salmon ({\textless}i{\textgreater}{Salmo} salar{\textless}i{\textgreater} {L}.) in seawater. {II}: effects on haematology and immunocompetence},
	volume = {37},
	abstract = {This research examined the haematological and immunological responses of quadruplicate groups of juvenile (similar to 400 g initial weight) Atlantic salmon (Salmo salar L.) that had each been fed daily to satiation for 12 weeks one of three high-energy extruded diets of identical composition except for the supplemental dietary lipid (234.7 g kg(-1)) source. The three experimental diets varied in the composition of supplement lipid; diet 1 contained 100{\textbackslash}\% anchovy oil (AO), while diets 2 and 3 replaced 29.8{\textbackslash}\% and 59.7{\textbackslash}\% of the AO (respectively) with a 1:1 blend of canola oil (CO) and poultry fat (PF). Immediately following the feeding trial, a random sample of fish from each diet was sampled for determination of baseline levels of various haematological and immunological parameters. Thereafter, duplicate diet groups were vaccinated (against Listonella anguillarum) and reared on their respective experimental diets for an additional 4 weeks. At that time, the remaining fish were sampled similarly, and the different parameters were measured again. Comparisons between the different diet treatment groups were made before and after vaccination. There were no significant diet treatment effects at either sample time, for haematocrit, differential leucocyte counts, erythrocyte counts, serum hemolytic activity or head kidney leucocyte respiratory burst activity. The fish fed diet 1 however, did show significantly higher post-vaccination levels of peripheral blood leucocyte respiratory burst activity and higher serum antibody titres against L. anguillarum. The results suggest that the relatively low n-6/n-3 fatty acid ratios in the muscle and presumably other tissues of fish fed diet 1, may have resulted in a reduced production of immunocompromising eicosanoids than were produced in fish ingesting the other two diets that were based in part on the different amounts of the CO and PF blend. Long-term studies are required to confirm this possibility.},
	number = {2},
	journal = {Aquaculture Research},
	author = {Balfry, S. K. and Oakes, J. and Rowshandeli, M. and Deacon, G. and Skura, B. J. and Higgs, D. A.},
	year = {2006},
	keywords = {Atlantic salmon; canola oil; poultry fat; immunocompetence; haematology; fatty acids TROUT ONCORHYNCHUS-MYKISS; ACID-COMPOSITION; RAINBOW-TROUT; ICTALURUS-PUNCTATUS; CHANNEL CATFISH; LEUKOCYTES},
	pages = {192--199},
}

