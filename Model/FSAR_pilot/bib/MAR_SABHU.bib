	keywords = {Stock assessment, Fisheries management, Proceedings, Stock identification, Symposium},
	pages = {1--8},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\F4IM2BYL\\Begg et al. - 1999 - Stock identification and its role in stock assessm.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\C4K5Q3HE\\S0165783699000624.html:text/html},
}

@article{beddington_current_2007,
