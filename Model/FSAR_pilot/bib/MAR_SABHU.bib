	keywords = {LIFE-HISTORY EVOLUTION; CONTEMPORARY MICROEVOLUTION; REACTION NORMS; GADUS-MORHUA; AGE; REPRODUCTION; POPULATIONS; FISHERIES; GROWTH; SIZE},
