	number = {5},
	urldate = {2019-06-19},
	journal = {Science Advances},
	author = {Steneck, Robert S. and Mumby, Peter J. and MacDonald, Chancey and Rasher, Douglas B. and Stoyle, George},
	month = may,
	year = {2018},
	pages = {eaao5493},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\LCA2ZVAS\\Steneck et al. - 2018 - Attenuating effects of ecosystem management on cor.pdf:application/pdf;Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\P3M6VKKT\\eaao5493.html:text/html},
