@article{beaugrand_marine_2010,
	title = {Marine biodiversity, ecosystem functioning, and carbon cycles},
	volume = {107},
	url = {10.1073/pnas.0913855107},
	abstract = {Although recent studies suggest that climate change may substantially accelerate the rate of species loss in the biosphere, only a few studies have focused on the potential consequences of a spatial reorganization of biodiversity with global warming. Here, we show a pronounced latitudinal increase in phytoplanktonic and zooplanktonic biodiversity in the extratropical North Atlantic Ocean in recent decades. We also show that this rise in biodiversity paralleled a decrease in the mean size of zooplanktonic copepods and that the reorganization of the planktonic ecosystem toward dominance by smaller organisms may influence the networks in which carbon flows, with negative effects on the downward biological carbon pump and demersal Atlantic cod (). Our study suggests that, contrary to the usual interpretation of increasing biodiversity being a positive emergent property promoting the stability/resilience of ecosystems, the parallel decrease in sizes of planktonic organisms could be viewed in the North Atlantic as reducing some of the services provided by marine ecosystems to humans.},
	number = {22},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Beaugrand, GrÃ©gory and Edwards, Martin and Legendre, Louis},
	year = {2010},
	pages = {10120--10124},
}

@article{bergenius_larval_2002,
	title = {Larval growth predicts the recruitment success of a coral reef fish},
	volume = {131},
