}

@article{simpfendorfer_environmental_2011,
	title = {Environmental {Influences} on the {Spatial} {Ecology} of {Juvenile} {Smalltooth} {Sawfish} ({Pristis} pectinata): {Results} from {Acoustic} {Monitoring}},
	volume = {6},
	shorttitle = {Environmental {Influences} on the {Spatial} {Ecology} of {Juvenile} {Smalltooth} {Sawfish} ({Pristis} pectinata)},
	url = {http://dx.doi.org/10.1371/journal.pone.0016918},
	doi = {10.1371/journal.pone.0016918},
	abstract = {To aid recovery efforts of smalltooth sawfish (Pristis pectinata) populations in U.S. waters a research project was developed to assess how changes in environmental conditions within estuarine areas affected the presence, movements, and activity space of this endangered species. Forty juvenile P. pectinata were fitted with acoustic tags and monitored within the lower 27 km of the Caloosahatchee River estuary, Florida, between 2005 and 2007. Sawfish were monitored within the study site from 1 to 473 days, and the number of consecutive days present ranged from 1 to 125. Residency index values for individuals varied considerably, with annual means highest in 2005 (0.95) and lowest in 2007 (0.73) when several P. pectinata moved upriver beyond detection range during drier conditions. Mean daily activity space was 1.42 km of river distance. The distance between 30-minute centers of activity was typically {\textless}0.1 km, suggesting limited movement over short time scales. Salinity electivity analysis demonstrated an affinity for salinities between 18 and at least 24 psu, suggesting movements are likely made in part, to remain within this range. Thus, freshwater flow from Lake Okeechobee (and its effect on salinity) affects the location of individuals within the estuary, although it remains unclear whether or not these movements are threatening recovery.},
	number = {2},
	urldate = {2013-12-16},
	journal = {PLoS ONE},
	author = {Simpfendorfer, Colin A. and Yeiser, Beau G. and Wiley, Tonya R. and Poulakis, Gregg R. and Stevens, Philip W. and Heupel, Michelle R.},
	month = feb,
	year = {2011},
	pages = {e16918},
	file = {PLoS Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\JEXWE9SU\\Simpfendorfer et al. - 2011 - Environmental Influences on the Spatial Ecology of.pdf:application/pdf},
