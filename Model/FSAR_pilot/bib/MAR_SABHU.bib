	keywords = {fisheries, marine protected area, Bycatch, Dermochelys coriacea, exclusion zone, Gabon, leatherback turtle, nesting},
	pages = {296--300},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\9SCA9K8G\\Witt et al. - 2008 - Satellite tracking highlights difficulties in the .pdf:application/pdf},
