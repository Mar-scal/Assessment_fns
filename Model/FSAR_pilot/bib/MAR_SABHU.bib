@article{richardson_sensitivity_2006,
	title = {Sensitivity of {Marine}-{Reserve} {Design} to the {Spatial} {Resolution} of {Socioeconomic} {Data}},
	volume = {20},
	issn = {1523-1739},
	url = {https://conbio.onlinelibrary.wiley.com/doi/abs/10.1111/j.1523-1739.2006.00426.x},
	doi = {10.1111/j.1523-1739.2006.00426.x},
	abstract = {Abstract: Socioeconomic considerations should have an important place in reserve design. Systematic reserve-selection tools allow simultaneous optimization for ecological objectives while minimizing costs but are seldom used to incorporate socioeconomic costs in the reserve-design process. The sensitivity of this process to biodiversity data resolution has been studied widely but the issue of socioeconomic data resolution has not previously been considered. We therefore designed marine reserves for biodiversity conservation with the constraint of minimizing commercial fishing revenue losses and investigated how economic data resolution affected the results. Incorporating coarse-resolution economic data from official statistics generated reserves that were only marginally less costly to the fishery than those designed with no attempt to minimize economic impacts. An intensive survey yielded fine-resolution data that, when incorporated in the design process, substantially reduced predicted fishery losses. Such an approach could help minimize fisher displacement because the least profitable grounds are selected for the reserve. Other work has shown that low-resolution biodiversity data can lead to underestimation of the conservation value of some sites, and a risk of overlooking the most valuable areas, and we have similarly shown that low-resolution economic data can cause underestimation of the profitability of some sites and a risk of inadvertently including these in the reserve. Detailed socioeconomic data are therefore an essential input for the design of cost-effective reserve networks.},
	language = {en},
	number = {4},
	urldate = {2020-03-31},
	journal = {Conservation Biology},
	author = {Richardson, Elizabeth A. and Kaiser, Michel J. and Edwards‐Jones, Gareth and Possingham, Hugh P.},
	year = {2006},
	pages = {1191--1202},
	file = {Richardson 2006-Sensitivity of Marine-Reserve Design to the Spatial Resolution of Socioeconomic-Conservation Biology.pdf:C\:\\Users\\keyserf\\Zotero\\storage\\HAN5UV7M\\Richardson 2006-Sensitivity of Marine-Reserve Design to the Spatial Resolution of Socioeconomic-Conservation Biology.pdf:application/pdf;Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\6FB94M3U\\j.1523-1739.2006.00426.html:text/html},
}

