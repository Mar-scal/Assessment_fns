@article{zuidema_persistently_2009,
	title = {Do {Persistently} {Fast} {Growing} {Juveniles} {Contribute} {Disproportionately} to {Population} {Growth}? {A} {New} {Analysis} {Tool} for {Matrix} {Models} and {Its} {Application} to {Rainforest} {Trees}.},
	volume = {174},
	url = {10.1086/605981},
	abstract = {Abstract: Plants and animals often exhibit strong and persistent growth variation among individuals within a species. Persistently fastâ€growing individuals have a higher chance of reaching reproductive size, do so at a younger age, and therefore contribute disproportionately to population growth (Î»). Here we introduce a new approach to quantify this â€œfastâ€growth effect.â€ We propose using ageâ€sizeâ€structured matrix models in which persistently fast and slow growers are distinguished as they occur in relatively young and old age classes for a given size category. Lifeâ€cycle pathways involving fast growth can then be identified, and their contribution to Î» is quantified through loop analysis. We applied this approach to an example species, the tropical rainforest tree Cedrela odorata, that shows persistent growth variation among individuals. Loop analysis showed that juvenile trees reaching the 10â€cm diameter class at belowâ€median age contributed twice as much to Î» as slow juvenile growers. Fast growth to largerâ€diameter categories also contributed disproportionately to Î». The results were robust to changes in parameter values and lifeâ€history tradeâ€offs. These results show that the fastâ€growth effect can be strong in longâ€lived species. Persistent growth differences among individuals should therefore be accommodated for in demographic models and lifeâ€history studies.},
	number = {5},
	journal = {The American Naturalist},
	author = {Zuidema, Pieter A. and Brienen, Roel J. W. and During, Heinjo J. and GÃ¼neralp, Burak},
	year = {2009},
	pages = {709--719},
}

