	keywords = {Ecology, Environmental science, fishery management, impact paper},
	pages = {386--389},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\NC5VSIHS\\Gutiérrez et al. - 2011 - Leadership, social capital and incentives promote .pdf:application/pdf},
