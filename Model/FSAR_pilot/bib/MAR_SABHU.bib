	keywords = {RAINBOW-TROUT; VITAMIN-C; ONCORHYNCHUS-KISUTCH; CHANNEL CATFISH; FISH; GROWTH; ASCORBATE-2-SULFATE; GAIRDNERI; DISEASE; CONVERSION},
	pages = {363--379},
}

@article{engen_reproductive_2009,
	title = {Reproductive {Value} and the {Stochastic} {Demography} of {Age} {Structured} {Populations}.},
	volume = {174},
	url = {10.1086/647930},
	abstract = {Abstract: The dynamics of an ageâ€structured population in a fluctuating environment is determined by the stochastic individual contributions from annual survival and fecundity to the total reproductive value of the population the next year. All parameters required to describe the population dynamics are simple properties of the distribution of these individual demographic contributions, which we call individual reproductive value. The asymptotic population growth rate in the average environment and the demographic and environmental variances are respectively the mean individual reproductive value over individuals through time and the variance within and between years. Our approach leads to an intuitive understanding of demographic and environmental variances in ageâ€structured populations and their decomposition into additive ageâ€specific components due to survival and reproduction. We show how to apply this approach to estimate the demographic and environmental variances and their components. The estimates are based on yearly random samples of individual vital rates and require no information about the total population size.},
	number = {6},
	journal = {The American Naturalist},
	author = {Engen, Steinar and Lande, Russell and SÃ¦ther, Berntâ€Erik and Dobson, F. Stephen},
	year = {2009},
	pages = {795--804},
}

