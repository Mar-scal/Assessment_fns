	pages = {152--170},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\CUHI8CML\\Link et al. - 2011 - Ecosystem-based fisheries management in the Northwest Atlantic .pdf:application/pdf;Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\4YGQ6UUN\\j.1467-2979.2011.00411.html:text/html},
