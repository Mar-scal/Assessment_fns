	year = {2015},
	pages = {575--588},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\6LLQW3E4\\Andersen et al. - 2015 - The theoretical foundations for size spectrum mode.pdf:application/pdf;Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\T89BSZPG\\cjfas-2015-0230.html:text/html},
}

@article{andres_measuring_2012,
