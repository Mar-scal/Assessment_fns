	author = {Andrés, Margarita and Prellezo, Raúl},
	month = jul,
	year = {2012},
	keywords = {{\textless}span class='italic'{\textgreater}Engraulis encrasicolus {\textless}/span{\textgreater}, Adaptability, Anchovy fishery closure, Atlantic Ocean, Cluster analysis, Fleet segments},
	pages = {205--214},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\BMCQFW6M\\Andrés and Prellezo - 2012 - Measuring the adaptability of fleet segments to a .pdf:application/pdf;Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\PELG9L46\\0420FFBF0D6E75A2C388AC933A68A806.html:text/html},
