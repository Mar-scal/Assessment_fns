@article{meadows_experimental_1989,
	title = {Experimental analysis of byssus thread production by {Mytilus} edulis and {Modiolus} modiolus in sediments},
	volume = {101},
	issn = {0025-3162, 1432-1793},
	url = {http://link.springer.com/10.1007/BF00391461},
	doi = {10.1007/BF00391461},
	abstract = {Laboratory experiments have been conducted on byssus thread production by two species of mussel, Mytilus eduIis L. and Modiolus rnodiolus (L.), representing the epibyssate and endobyssate species of the Mytilidae, respectively. Mussels were placed in seven particle sizes of sediment ranging from 50/zm to 16 mm for 12 d. The number of byssus threads per mussel, length of threads, number of threads per particle and size of pads were then recorded. Modiolus rnodiolus (endobyssate) produced more threads than Mytilus edulis (epibyssate). M. edulis produced most threads in the size range 2 to 16 mm and Modiolus modiolus in the size range 500 to 1000 lzm. M. rnodiolus produced longer threads than Mytilus edulis. Both species produced longer threads in particle sizes finer than 2 ram. M. edulis produced smaller pads than Modiolus rnodiolus. Both species produced larger pads in sediments coarser than 1 000/an. Results were also analysed in terms of attachment units (a thread attached to one or more particles, or a particle to which more than one thread is attached). The thread:particle ratio of the attachment units ranged from ca. 1 : 10 in the fmest sediments to ca. 10 : 1 in the coarsest sediments. Mytilus edulis attached more threads to single particles (72\% of attachment units) than Modiolus rnodiolus (37\% of attachment units). M. rnodiolus had a wider spread of ratios in the finer particle sizes than Mytilus edulis. The sigmificance of our results are discussed in relation to the ecology and palaeoecology of epibyssate and endobysste species.},
	language = {en},
	number = {2},
	urldate = {2018-11-22},
	journal = {Marine Biology},
	author = {Meadows, P. S. and Shand, P.},
	month = apr,
	year = {1989},
	pages = {219--226},
	file = {Meadows and Shand - 1989 - Experimental analysis of byssus thread production .pdf:C\:\\Users\\keyserf\\Zotero\\storage\\PZPC9NDC\\Meadows and Shand - 1989 - Experimental analysis of byssus thread production .pdf:application/pdf},
