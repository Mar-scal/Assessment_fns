@article{xu_nonlinear_2014,
	title = {Nonlinear {Mixed}-{Effects} ({NLME}) {Diameter} {Growth} {Models} for {Individual} {China}-{Fir} ({\textless}i{\textgreater}{Cunninghamia} lanceolata{\textless}i{\textgreater}) {Trees} in {Southeast} {China}},
	volume = {9},
	issn = {1932-6203},
	url = {https://dx.plos.org/10.1371/journal.pone.0104012},
	doi = {10.1371/journal.pone.0104012},
	abstract = {An individual-tree diameter growth model was developed for Cunninghamia lanceolata in Fujian province, southeast China. Data were obtained from 72 plantation-grown China-fir trees in 24 single-species plots. Ordinary non-linear least squares regression was used to choose the best base model from among 5 theoretical growth equations; selection criteria were the smallest absolute mean residual and root mean square error and the largest adjusted coefficient of determination. To account for autocorrelation in the repeated-measures data, we developed one-level and nested two-level nonlinear mixedeffects (NLME) models, constructed on the selected base model; the NLME models incorporated random effects of the tree and plot. The best random-effects combinations for the NLME models were identified by Akaike’s information criterion, Bayesian information criterion and 22 logarithm likelihood. Heteroscedasticity was reduced with two residual variance functions, a power function and an exponential function. The autocorrelation was addressed with three residual autocorrelation structures: a first-order autoregressive structure [AR(1)], a combination of first-order autoregressive and moving average structures [ARMA(1,1)] and a compound symmetry structure (CS). The one-level (tree) NLME model performed best. Independent validation data were used to test the performance of the models and to demonstrate the advantage of calibrating the NLME models.},
	language = {en},
	number = {8},
	urldate = {2018-11-22},
	journal = {PLoS ONE},
	author = {Xu, Hao and Sun, Yujun and Wang, Xinjie and Fu, Yao and Dong, Yunfei and Li, Ying},
	editor = {Wu, Rongling},
	month = aug,
	year = {2014},
	pages = {e104012},
	file = {Xu et al. - 2014 - Nonlinear Mixed-Effects (NLME) Diameter Growth Mod.PDF:C\:\\Users\\keyserf\\Zotero\\storage\\JEMETTLD\\Xu et al. - 2014 - Nonlinear Mixed-Effects (NLME) Diameter Growth Mod.PDF:application/pdf},
}

@article{yochum_size-selectivity_2008,
	title = {Size-{Selectivity} of the {Northwest} {Atlantic} {Sea} {Scallop} ({\textless}i{\textgreater}{Placopecten} magellanicus{\textless}i{\textgreater}) {Dredge}},
	volume = {27},
	issn = {0730-8000, 1943-6319},
	url = {https://bioone.org/journals/journal-of-shellfish-research/volume-27/issue-2/0730-8000_2008_27_265_SOTNAS_2.0.CO_2/Size-Selectivity-of-the-Northwest-Atlantic-Sea-Scallop-Placopecten-magellanicus/10.2983/0730-8000(2008)27[265:SOTNAS]2.0.CO;2.full},
	doi = {10.2983/0730-8000(2008)27[265:SOTNAS]2.0.CO;2},
	abstract = {A size-selectivity curve was constructed to characterize the performance of the New Bedford style Atlantic sea scallop (Placopecten magellanicus, Gmelin 1791) dredge when it is configured to meet the requirements of Amendment \#10 to the Sea Scallop Fishery Management Plan. The curve was generated using the SELECT model on catch-at-length data, obtained by simultaneously towing a New Bedford style dredge and a nonselective National Marine Fisheries Service sea scallop survey dredge from commercial scallop vessels. Data were collected during three cruises in the Northwest Atlantic between 2005 and 2006. The resultant selectivity curve yielded a 50\% retention length of 100.1 mm, a selection range of 23.6 mm, and a value of 0.77 for the efficiency of the commercial dredge relative to the survey dredge. A length of 100.1 mm corresponds to a meat weight of 17.2 g in Georges Bank and 16.8 g in the mid-Atlantic. These results can assist fisheries managers with stock assessments, fishing mortality estimates, and the interpretation of catch data from resource surveys. Additionally, the curve can be used as a baseline to evaluate the effect of future changes to sea scallop dredge configuration.},
	number = {2},
	urldate = {2021-08-27},
	journal = {Journal of Shellfish Research},
	author = {Yochum, Noëlle and Dupaul, William D.},
	month = apr,
	year = {2008},
	note = {Publisher: National Shellfisheries Association},
	pages = {265--271},
	file = {Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\CI2MJ8X2\\0730-8000(2008)27[265SOTNAS]2.0.CO\;2.html:text/html;Yochum and Dupaul - 2008 - Size-Selectivity of the Northwest Atlantic Sea Sca.pdf:C\:\\Users\\keyserf\\Zotero\\storage\\U9JY38ZR\\Yochum and Dupaul - 2008 - Size-Selectivity of the Northwest Atlantic Sea Sca.pdf:application/pdf},
}

@article{thompson_identifying_2014,
	title = {Identifying {Spawning} {Events} of the {Sea} {Scallop} {\textless}i{\textgreater}{Placopecten} magellanicus{\textless}i{\textgreater} on {Georges} {Bank}},
	volume = {33},
	issn = {0730-8000, 1943-6319},
	url = {https://bioone.org/journals/Journal-of-Shellfish-Research/volume-33/issue-1/035.033.0110/Identifying-Spawning-Events-of-the-Sea-Scallop-iPlacopecten-magellanicus-i/10.2983/035.033.0110.full},
	doi = {10.2983/035.033.0110},
	abstract = {Georges Bank is the most productive sea scallop fishing ground in the world, but little is known about the regional spawning patterns. The sea scallop rotational management plan is based on yield projections estimated from shell height/meat weight relationships. Semiannual spawning may influence yield projections, impacting fishery allocations. This study identifies spawning events at two locations on Georges Bank: one in Closed Area I (CAI) and the other in Closed Area II (CAII). We hypothesize that spawning is semiannual, spring spawning is incomplete compared with autumn spawning, and reproductive stage differs temporally between areas. Scallops (n = 1,871) were collected during a monthly dredge survey in these two areas from two sites from March 2011 through June 2013. Tissues from scallops (shell height ∼130 mm) frozen at sea, were oven-dried and the gonosomatic indices (GSIs) were analyzed to identify spawning events. Oocyte diameter was measured to determine maturity. Bottom temperature was recorded. Semiannual spawning occurred in both closed areas. Spawning rates were similar, but autumn spawning was greater in magnitude than spring spawning. The timing of gametogenesis was similar between sites. Bottom temperature patterns suggest different oceanographic conditions between areas. A semiannual sea scallop reproductive cycle on Georges Bank could influence recruitment and growth assumptions affecting future management decisions.},
	number = {1},
	urldate = {2019-06-05},
	journal = {Journal of Shellfish Research},
	author = {Thompson, Katherine J. and Inglis, Susan D. and Stokesbury, Kevin D. E.},
	month = apr,
	year = {2014},
	pages = {77--87},
	file = {Full Text PDF:C\:\\Users\\keyserf\\Zotero\\storage\\XH232YUI\\Thompson 2014 - Identifying Spawning events of the sea scallop on GB.pdf:application/pdf;Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\J5MCHSLA\\035.033.0110.html:text/html},
