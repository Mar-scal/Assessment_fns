	author = {Stamoulis, Kostantinos A. and Delevaux, Jade M. S.},
	month = nov,
	year = {2015},
	keywords = {Spatial management, Coastal and marine spatial planning, Cumulative impact assessment, Data, Decision support systems, Ecological modeling, Ecosystem-based management, Geographic information system, Human dimensions},
	pages = {214--223},
	file = {ScienceDirect Snapshot:C\:\\Users\\keyserf\\Zotero\\storage\\AKRPB8QZ\\S096456911500201X.html:text/html;Stamoulis 2015-Data requirements and tools to operationalize marine spatial planning in the-Ocean & Coastal Management.pdf:C\:\\Users\\keyserf\\Zotero\\storage\\9SHDRP8J\\Stamoulis 2015-Data requirements and tools to operationalize marine spatial planning in the-Ocean & Coastal Management.pdf:application/pdf},
